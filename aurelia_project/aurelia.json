{
  "name": "casl-aurelia-example",
  "type": "project:application",
  "paths": {
    "root": "src",
    "resources": "components",
    "elements": "components",
    "components": "components",
    "attributes": "components",
    "valueConverters": "pipes",
    "bindingBehaviors": "binding-behaviors"
  },
  "transpiler": {
    "id": "babel",
    "fileExtension": ".js"
  },
  "build": {
    "options": {
      "server": "dev",
      "extractCss": "prod",
      "coverage": false
    }
  },
  "platform": {
    "hmr": false,
    "open": false,
    "port": 8080,
    "host": "localhost",
    "output": "dist"
  },
  "packageManager": "npm"
}
